{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "eslint-config-prettier"
  ],
  "globals": {
    "React": "writable"
  },
  "rules": {
    "strict": ["error", "never"],
    "react/react-in-jsx-scope": "off"
  },
  "env": {
    "browser": true
  }
}
